(function() {
	function addEvent(obj, eventType, func) {
		if(obj.attachEvent) {
			obj.attachEvent("on" + eventType, func);
		} else {
			obj.addEventListener(eventType, func, false);
		}
	};

	function attachURL2Window(id) {
		var links;		
		try {
			links = document.getElementById(id).getElementsByTagName("a");
		}catch(e) {
			links = [];
		}

		for (var i = 0, len = links.length; i < len; i++) {
			if (/sina\.com\.cn.*?\/2010[\-\/]?\d{2}[\-\/]?\d{2}/.test(links[i].href)) {
				addEvent(links[i], "mousedown", function(e) {
					var w = window, evt = e || window.event;
					var target = evt.target || evt.srcElement;
					var url = target.href;
					url = url.replace(/\s*$/, "").replace(/^\s*/, "");
					w.cancelMiniBlog = w.cancelMiniBlog || {};
					w.cancelMiniBlog[url] = true;
				});
			}
		}
	};	
	attachURL2Window("news_con_1");
})()
