def write_script(filename):
        try:
	    name = filename+"script"
            fd = open('log/'+ name, 'wb')
            fd.write(self.script + ';'+'\n')
            fd.close()
	    print "script written to scripts"
	except IOError:
	    pass

config.write_script(hashlib.md5(window.document.location.href).hexdigest())

